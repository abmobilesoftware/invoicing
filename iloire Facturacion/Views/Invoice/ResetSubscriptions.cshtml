@model List<KeyValuePair<string,string>>

@{
    ViewBag.Title = "Reset subscriptions";
}

<h2>Reset Subscriptions Result</h2>

<table class="zebra-striped">
     <thead>
        <th width="20%">Company</th>
        <th>Result</th>
     </thead>
   @foreach (var item in Model)
   {
      <tr>
         <td>@item.Key</td>         
         @if (@item.Value != iloire_Facturacion.Controllers.InvoiceController.cSuccess)
         {
            <td style="color: red;font-weight: bold;">@item.Value</td>   
         }
         else
         {
             <td style="color: green;">@item.Value</td>             
         }
      </tr>
   }
</table>
